{
  "name": "GEOFLUXUS",
  "version": "1.0.0",
  "description": "NLG Bootstrap4 theme kit for modx",
  "fonts": {
    "sansserif": "Open+Sans:300,300i,400,400i,700,700i"
  },
  "fonts_subset": "latin-ext",
  "sync": {
    "browser": false,
    "browser_online": false,
    "ftp": false,
    "fonts": false,
    "fonts_local": false,
    "presets": false,
    "vendors": false,
    "bootstrap": false,
    "extras": true,
    "min": true,
    "js": true,
    "css": true,
    "img": true,
    "media": true
  },
  "server": {
    "url": "http://geofluxus.com",
    "host": "ftp.geofluxus.com",
    "folder": "/public_html/",
    "port": "21",
    "secure": false
  },
  "login": {
    "user": "",
    "password": ""
  },
  "server_options": {
    "parallel": 3,
    "maxConnections": 5
  },
  "js_compile": {
    "core": "core",
    "plugins": "plugins",
    "elements": "elements"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brezni/theme-kit.git"
  },
  "homepage": "https://github.com/geoFluxus/geofluxus.com#readme",
  "bugs": {
    "url": "https://github.com/geoFluxus/geofluxus.com/issues"
  },
  "author": "NOLOGO",
  "license": "MIT",
  "keywords": [
    "bootstrap4",
    "modx",
    "nlg"
  ],
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 10"
  ],
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "bootstrap": "^4.2.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "browser-sync": "^2.26.7",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-clean-css": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-google-webfonts": "^2.1.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.1.1",
    "gulp-order": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^1.2.0",
    "gulp-touch": "^1.0.1",
    "gulp-wait": "0.0.2",
    "gulp-zip": "^5.0.1",
    "run-sequence": "^2.2.1",
    "vinyl-ftp": "^0.6.1"
  }
}
